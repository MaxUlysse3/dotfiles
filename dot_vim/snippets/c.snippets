snippet main
	#include <stdio.h>
	#include <stdlib.h>

	int main(${1:void}) {
		$2
	}
snippet for
	for (int i = 0;i < $1; i++) {
		$2
	}
snippet forc
	for (${1:int} ${2:i} = ${3:0};$2 ${4:<} $5;${6:$2++}) {
		$7
	}
snippet if
	if ($1) {
		$2
	}
snippet def
	$1 $2($3) {
		$4
	}
snippet print
	printf("${1:%d}\\n", $2);$3
